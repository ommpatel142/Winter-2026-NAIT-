#include<stdio.h>
int main(){
    int x;
    int y=2;
    int a=0;
    printf("enter a number\n");
    scanf("%d",&x);
    
    while (y<x) {
        if(x%y==0){
            a++;
        }
        y++;
     }
     if (a==0) {
         printf("the entered number is prime");
     }
     else {
         printf("the entered number is not prime");
     }
   
}
    
    
