#include<stdio.h>
int main(){
    int x;
    int y;
    
    printf("enter starting number\n");
    scanf("%d",&x);
    printf("enter ending number\n");
    scanf("%d",&y);
    
    
    
    while(x<=y) {
       if(x%2==0) {
           printf("even = %d\n",x);
       }
       else {
           printf("odd = %d\n",x);
       }
       x++;
    }
    
   
}
